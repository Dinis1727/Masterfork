{
  "name": "masterfork-server",
  "version": "0.1.0",
  "private": true,
  "description": "Express server for handling orders with Prisma tooling",
  "type": "commonjs",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:generate": "prisma generate",
    "prisma:seed": "prisma db seed"
  },
  "dependencies": {
    "@prisma/adapter-pg": "7.1.0",
    "@prisma/client": "7.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pg": "^8.11.5"
  },
  "devDependencies": {
    "prisma": "7.1.0"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
